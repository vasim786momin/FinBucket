<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.fin.kekanepc.finbucket.TermCondition"
    android:background="#3b3347">
    <com.michaldrabik.tapbarmenulib.TapBarMenu
        android:id="@+id/tapBarMenu"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        app:tbm_buttonSize="40dp"
        app:tbm_showItems="false"
        app:tbm_backgroundColor="#00ffffff"
        app:tbm_buttonPosition="left"
        app:tbm_menuAnchor="top"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:id="@+id/item1"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:layout_weight="1"
            android:paddingBottom="3dp"
            android:layout_marginLeft="45dp"
            android:src="@drawable/homeee"
            />
        <ImageView
            android:id="@+id/item2"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:layout_weight="1"
            android:paddingBottom="3dp"
            android:src="@drawable/tc"
            />

        <ImageView
            android:id="@+id/item3"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:layout_weight="1"
            android:paddingBottom="3dp"
            android:src="@drawable/contactus"
            />

        <ImageView
            android:id="@+id/item4"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:layout_weight="1"
            android:paddingBottom="3dp"
            android:src="@drawable/aboutus"
            />
    </com.michaldrabik.tapbarmenulib.TapBarMenu>
</RelativeLayout>
